<Page x:Class="Soloplan.WhatsON.GUI.Config.SubjectsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Soloplan.WhatsON.GUI.Config"
      mc:Ignorable="d" 
      d:DesignHeight="450"
      Title="SubjectsPage" MinWidth="300">

  <Grid Margin="0">
    <DockPanel>
      <ComboBox x:Name="uxSubjects" Height="23" DockPanel.Dock="Top" SelectionChanged="UxSubjectsSelectionChanged"/>
      <Frame x:Name="uxSubjectFrame" Content="Frame" DockPanel.Dock="Bottom"/>
    </DockPanel>
  </Grid>
</Page>
